Public Class Booking
    Public Property BookingId As Integer
    Public Property LimousineModel As String
    Public Property LimousineStyle As String
    Public Property PickupDate As Date
    Public Property DropoffDate As Date

    Public Sub New(ByVal BookingId As Integer, ByVal LimousineModel As String, ByVal LimousineStyle As String, ByVal PickupDate As Date, DropoffDate As Date)
        Me.BookingId = BookingId
        Me.LimousineModel = LimousineModel
        Me.LimousineStyle = LimousineStyle
        Me.PickupDate = PickupDate
        Me.DropoffDate = DropoffDate
    End Sub
End Class
